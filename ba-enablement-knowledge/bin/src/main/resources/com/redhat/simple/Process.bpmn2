      <bpmn2:outgoing>SequenceFlow_2</bpmn2:outgoing>
      <bpmn2:script>System.out.println( &quot;I loaded my business, but have not filtered it&quot; );
kcontext.getKnowledgeRuntime().insert( kcontext.getProcessInstance().getVariable( &quot;Business&quot; ) );
</bpmn2:script>
    </bpmn2:scriptTask>
